/*Ordernando por mais uma coluna*/

select * from cliente 
order by 1, 2;

/*MESCALANDO ORDER BY COM PROJECAO*/

select * from cliente ?/*erro*/
order by 1, 2;

select NOME from cliente 
order by nome;


/*Order by desc /asc*/
select * from cliente 
order by 1 asc; /*Order crescente*/

select * from cliente 
order by 1 desc; /*Order Decrescente*/


select * from cliente 
order by 1 desc ,2;


/*Ordenando com join*/

SELECT
c.NOME,
c.SEXO, 
IFNULL(c.EMAIL, "--NOT FOUND--"), 
t.NUMERO, 
e.BAIRRO, 
e.CIDADE, 
e.ESTADO
FROM CLIENTE c
inner join telefone t
on c.idCliente = t.id_Cliente
inner join endereco e 
on c.idCliente = e.id_Cliente
order by 1 desc;

/* TROCANDO O DELIMITADOR*/
delimiter $ /*APENAS UM ESPACO ANTES DO DELIMITADOR*/

/*em Status mostra qual o demilitirador*/
Using delimiter:        $

/*FECHANDO O BANCO O DEMILITADOR VOLTA AO PADRAO ; */

/*--------------------------------------------------------------------------*/

/*STORED PROCEDURES - PROCEDIMENTOS ARMAZENADOS NO BANCO*/

DELIMITER $


// É NECESSARIO MUDAR O DELIMITADOR PORQUE DENTRO DA PROCEDURE UTILIZA ';' COMO FECHAMENTO, 
//CASO NAO ALTERE O BANCO IRA SE CONFUNDIR, E IRA CAUSAR UM STOP NESSE FECHAMENTO, COM A PROCEDURE INCOMPLETA

CREATE PROCEDURE NOME()
BEGIN

QUALQUER PROGRAMACAO; 

END
$

CREATE PROCEDURE NOME_EMPRESA()
BEGIN

SELECT 'UNIVERSIDADE DOS DADOS'  AS EMRPESA;

END
$

/*CHAMANDO UMA PROCEDURE*/

CALL NOME_EMPRESA()$

DELIMITER ;

CALL NOME_EMPRESA(); // APOS CRIAR A PROCEDURE PODE VOLTAR COM O DELIMITER NORMAL E CHAMALO


DELIMITER $
CREATE PROCEDURE CONTA()
BEGIN
SELECT 10 + 10 AS CONTA;
END
$

DELIMITER ;
CALL CONTA;
CALL CONTA();

DROP PROCEDURE CONTA;

DELIMITER $
CREATE PROCEDURE CONTA(NUMERO1 INT, NUMERO2 INT)
BEGIN
SELECT NUMERO1 + NUMERO2 AS CONTA;
END
$

call conta(150, 1000);
