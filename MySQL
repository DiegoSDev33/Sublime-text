+----------------+------+-------------------+------+------------+

select cliente.nome, cliente.sexo, endereco.bairro, endereco.cidade, telefone.tipo, telefone.numero
from cliente 
inner join Endereco 
on endereco.id_Cliente = cliente.idCliente
inner join telefone
on telefone.id_Cliente = cliente.idCliente;

