/*TRABALHANDO COM VIEWS*/

SELECT c.NOME, c.SEXO, c.EMAIL, t.NUMERO, e.BAIRRO, e.CIDADE, e.ESTADO
FROM CLIENTE c
inner join telefone t
on c.idCliente = t.id_Cliente
inner join endereco e 
on c.idCliente = e.id_Cliente;

CREATE VIEW V_RELATORIO AS SELECT
c.NOME,
c.SEXO, 
IFNULL(c.EMAIL, "--NOT FOUND--"), 
t.NUMERO, 
e.BAIRRO, 
e.CIDADE, 
e.ESTADO
FROM CLIENTE c
inner join telefone t
on c.idCliente = t.id_Cliente
inner join endereco e 
on c.idCliente = e.id_Cliente;

